    "manifest_version": 3,
    "background":{
        "service_worker": "./background.js"
    },
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "https://acquia.lightning.force.com/lightning/r/Case/*"
    ]
}